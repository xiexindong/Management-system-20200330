
{
  "presets": [
    ["@babel/preset-env",{
      "targets":{
        "edge": "17",
        "firefox": "60",
        "chrome": "67",
        "safari": "11.1",
        "ie":"10"
      },
      "useBuiltIns":"usage",
      "corejs": "3"
    }],
    ["@babel/preset-react"]

  ],
  "plugins":[
    ["@babel/plugin-proposal-class-properties"],
    ["import",{
      "libraryName": "antd",
      "libraryDirectory": "es",
      "style": "css" // `style: true` 会加载 less 文件
    }]
  ]
}